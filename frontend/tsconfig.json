{
    "compilerOptions": {
        "target": "es2015",
        "jsx": "react",
        "noEmit": true,
        "moduleResolution": "node",
        "noImplicitAny": true,
        "allowSyntheticDefaultImports": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "rootDir": ".",
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules",
        "*/**.spec.ts"
    ]
}
