{
    "defaultSeverity": "error",
    "extends": [
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "object-literal-sort-keys": false,
        "no-empty-interface": false,
        "linebreak-style": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "__tests__/**"
        ]
    }
}