{
  "name": "ConversationalTax",
  "version": "0.0.4",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "exp start",
    "android": "exp android",
    "ios": "exp ios",
    "publish": "exp publish",
    "test": "jest",
    "lint": "tslint -c tslint.json --project tsconfig.json"
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "autobind-decorator": "^2.1.0",
    "expo": "^27.0.1",
    "react": "^16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-27.0.1.tar.gz",
    "react-navigation": "^1.5.11"
  },
  "devDependencies": {
    "@types/jest": "22.2.3",
    "@types/react": "^16.3.12",
    "@types/react-native": "^0.55.10",
    "@types/react-navigation": "^1.5.9",
    "@types/react-test-renderer": "16.0.1",
    "babel-jest": "22.4.3",
    "eslint": "^4.19.1",
    "exp": "^53.1.0",
    "jest": "22.4.3",
    "jest-expo": "^27.0.0",
    "react-native-typescript-transformer": "^1.2.5",
    "react-test-renderer": "^16.3.2",
    "ts-jest": "^22.4.4",
    "tslint": "5.9.1",
    "tslint-eslint-rules": "5.1.0",
    "tslint-react": "3.5.1",
    "typescript": "^2.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation))"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "android.ts",
      "android.tsx"
    ]
  },
  "babel": {
    "presets": [
      "babel-preset-expo"
    ],
    "env": {
      "development": {
        "plugins": [
          "transform-react-jsx-source"
        ]
      }
    }
  }
}
