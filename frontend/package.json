{
  "name": "ConversationalTax",
  "version": "0.0.4",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "preinstall": "cd ../shared && npm i",
    "start": "exp start",
    "android": "exp android",
    "ios": "exp ios",
    "publish": "exp publish",
    "build": "tsc",
    "test": "node_modules/.bin/tsc && jest",
    "lint": "tslint -c tslint.json --project tsconfig.json"
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "autobind-decorator": "^2.1.0",
    "conv-tax-shared": "file:../shared/",
    "expo": "^28.0.0",
    "react": "^16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-28.0.0.tar.gz",
    "react-native-tab-view": "^1.0.2",
    "react-navigation": "^1.5.12",
    "rxjs": "^6.2.1",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "@types/expo": "^27.0.2",
    "@types/expo__vector-icons": "^6.2.2",
    "@types/jest": "^23.1.0",
    "@types/react": "^16.3.1",
    "@types/react-native": "^0.55.4",
    "@types/react-navigation": "^1.5.12",
    "@types/react-test-renderer": "16.0.1",
    "@types/superagent": "^3.8.0",
    "babel-jest": "^23.0.1",
    "eslint": "^4.19.1",
    "exp": "^55.0.2",
    "jest": "^23.1.0",
    "jest-expo": "^28.0.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "^16.4.1",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation))"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "android.ts",
      "android.tsx"
    ]
  },
  "babel": {
    "presets": [
      "babel-preset-expo"
    ],
    "env": {
      "development": {
        "plugins": [
          "transform-react-jsx-source"
        ]
      }
    }
  }
}
